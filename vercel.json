{
  "version": 2,
  "builds": [
    {
      "src": "src/api.ts",
      "use": "@vercel/node",
      "config": {
        "installCommand": "pnpm install --no-frozen-lockfile"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/api.ts"
    }
  ]
}